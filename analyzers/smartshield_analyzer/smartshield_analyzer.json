{
    "name": "smartshield_analyzer",
    "displayName": "SmartShield Analyzer",
    "version": "1.0",
    "author": "IEEE-EPS",
    "url": "https://github.com/ieee-eps/smartshield",
    "license": "AGPL-v3",
    "description": "Analyzer that uses the SmartShield AI model",
    "dataTypeList": ["ip", "domain", "url", "hash", "mail"],
    "baseConfig": "CortexAnalyzers",
    "configurationItems": {
      "url": {
        "type": "string",
        "description": "URL of the SmartShield AI model API",
        "required": true,
        "default": "http://localhost:8080"
      },
      "api_key": {
        "type": "string",
        "description": "API key for authentication",
        "required": true
      },
      "verify_ssl": {
        "type": "boolean",
        "description": "Verify SSL certificates",
        "required": false,
        "default": true
      }
    }
  }
  